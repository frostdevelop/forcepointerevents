{
  "manifest_version": 3,
  "name": "ForcePointerEvents",
  "version": "1.0",
  "description": "Forces pointer events onto images!",
  "action": {
    "default_title": "Force Pointer Events!",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
